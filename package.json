    "vuetify": "^1.5.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "to-string-loader": "^1.1.5",
    "vue-cli-plugin-vuetify": "^0.4.6",
    "vue-loader": "^15.6.4",
    "vue-template-compiler": "^2.6.6",
    "vuetify-loader": "^1.2.1"
  },
